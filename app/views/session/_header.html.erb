<% if log_in? == false %>
  <%= link_to 'Sign Up',  new_user_path, class: "btn btn-primary"  %>
  <%= link_to 'Login',  login_path, class: "btn btn-primary"  %>
<% else %>
  <%= link_to 'Logout',logout_path, method: :delete, data: {confirm: 'Do you want to log out?'} , class: "btn btn-primary"%>
<% end %>
