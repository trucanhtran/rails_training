<div>
  <%= form_with model: @item_store, url: store_create_path do |f| %>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name, id: "id_name", class: "name" %>
    </div>
    <div>
      <%= f.label :price %>
      <%= f.number_field :price, id: "id_price", class: "price" %>
    </div>
    <div>
      <%= f.label :category_id %>
      <%= f.number_field :category_id, id: "id_category", class: "id-category" %>
    </div>
    <div>
       <%= f.submit :submit %>
    </div>
  <% end %>
</div>
